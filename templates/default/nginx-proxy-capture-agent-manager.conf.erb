    location / {
        proxy_buffering off;
        proxy_pass http://unix:/home/<%= @capture_agent_manager_username %>/sock/<%= @capture_agent_manager %>.sock;
    }

    location /static {
    alias /home/<%= @capture_agent_manager_username %>/sites/<%= @capture_agent_manager %>/<%= @capture_agent_manager %>/static;
        autoindex on;
    }

